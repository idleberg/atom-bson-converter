{
  "name": "bson-converter",
  "description": "Converts BSON into JSON (or CSON) and vice versa",
  "version": "0.4.2",
  "license": "MIT",
  "contributors": [
    {
      "name": "Jan T. Sott",
      "url": "https://github.com/idleberg"
    }
  ],
  "repository": "https://github.com/idleberg/atom-bson-converter",
  "keywords": [
    "bson",
    "cson",
    "json",
