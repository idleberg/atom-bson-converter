    "cson": "^4.1.0"
  },
  "devDependencies": {
    "coffeelint-stylish": "^0.1.2",
    "gulp": "^4.0.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^3.1.0",
    "gulp-jsonlint": "^1.2.0",
    "husky": "^0.14.3"
  },
  "scripts": {
    "test": "gulp lint"
  }
}
